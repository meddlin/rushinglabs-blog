{
  "name": "rushinglabs-site",
  "version": "0.3.0-beta",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --collectCoverage --",
    "lint": "next lint",
    "megalint": "mega-linter-runner -f cupcake",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "debug-test-unix": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "debug-test-windows": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "build": "next build",
    "start": "next start -p $PORT",
    "export": "next export"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@next/mdx": "^13.0.3",
    "@testing-library/react": "^14.1.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.17.0",
    "escape-html": "^1.0.3",
    "gray-matter": "^4.0.2",
    "next": "^14.0.2",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "^15.4.5",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "autoprefixer": "^10.4.13",
    "eslint": "8.46.0",
    "eslint-config-next": "^13.4.13",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mega-linter-runner": "^7.1.0",
    "next-mdx-remote": "^4.2.0",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4"
  }
}
