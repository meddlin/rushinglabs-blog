{
  "name": "rushinglabs-site",
  "version": "0.2.0-beta",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --collectCoverage --",
    "lint": "next lint",
    "megalint": "mega-linter-runner -f cupcake",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "debug-test-unix": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "debug-test-windows": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "build": "next build",
    "start": "next start -p $PORT",
    "export": "next export"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^12.0.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.17.0",
    "gray-matter": "^4.0.2",
    "next": "^12.0.4",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "^15.4.5",
    "sharp": "^0.30.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mega-linter-runner": "^6.14.0",
    "next-mdx-remote": "^4.2.0"
  }
}
